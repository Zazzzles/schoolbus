    "lint:js": "eslint --fix src",
    "lint:styles": "stylelint --fix src/**/*.js",
    "lint": "npm run lint:js && npm run lint:styles",
    "fmt": "prettier-eslint --write \"src/**/*.js\"",
    "build": "npm run build:transpile:cjs && npm run build:transpile:esm && npm run build:postcss",
    "build:transpile:cjs": "rm -rf lib && cross-env BABEL_MODULE=commonjs babel src --out-dir lib",
    "build:transpile:esm": "rm -rf lib-esm && babel src --out-dir lib-esm",
    "build:postcss": "cat src/index.css | postcss -u postcss-import > ./lib/index.css && cp ./lib/index.css ./lib-esm/index.css",
    "prepare": "npm run build",
    "chromatic": "chromatic --exit-zero-on-changes --project-token=0r1m4x1jq86",
    "test": "jest"
  },
